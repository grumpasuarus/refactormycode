<% @title = "Edit #{@code.title}" %>
<h1><%=h @title %></h1>

<%= error_messages_for :code %>

<% form_for :code, :url => code_path(@code), :html => { :method => :put } do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%= submit_tag "Update", :disable_with => 'Updating ...' %>
    or
    <%= link_to 'Cancel', code_path(@code), :class => 'cancel' %>
  </p>
<% end %>

<%= link_to 'Delete this submission', code_path(@code), :confirm => 'Are you sure?', :method => :delete %>